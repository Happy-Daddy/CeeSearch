/* ================================ */
.my_tab{
  display: block;
  height: 80rpx;
  border: solid #00bea1 2rpx;
  border-radius: 20rpx;
  margin: 30rpx;

}

.my_tab > view{
  float: left;
  text-align: center;
  height: 78rpx;
  font-size: 32rpx;
  line-height: 80rpx;
  color: #00bea1;
  border-right: solid #00bea1 2rpx;
}

.my_tab view:first-child{
  border-radius: 18rpx 0 0 18rpx;
}


.my_tab > view.selected{
  background: #00bea1;
  color: #fff;
}


.my_tab view:last-child{
  border-right: none;
  border-radius:0 18rpx 18rpx 0;
}

.my_tab.my_tab_col_2 > view{
  width: 50%;
}

.my_tab.my_tab_col_4 > view{
  width: 25%;
}
.my_tab.my_tab_col_3 > view{
  width: 33.3333%;
}


/* bottom_bar======================= */
.bottom_bar{
  position: fixed;
  bottom: 0;
  width: 100%;
  display: block;
  background: #f9f9f9; border-top: solid #ccc 1rpx;
  height: 110rpx;
}

.bottom_bar view{
  float: left;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 30rpx;
  margin: 20rpx 0;
  color: #999;
}
.bottom_bar view text{
  background-repeat: no-repeat;
  background-position: left center;
  padding-left: 45rpx;
  background-size: 35rpx;
}
.bottom_bar view:first-child{
  border-right: solid #ccc 1rpx;
}

.bottom_bar.bottom_bar_col_2 view{
  width: 50%;
}

/* ratio============================= */
.students{
  text-align: center;
  width: 100%;
}

.ratio{
  width: 100%;
  display: block;
  height: 60rpx;
  background: #ffb5ba;
  border-radius: 30rpx;
  position: relative;
  text-align: center;
}

.ratio view{
  background: #64ceec;
  border-radius: 30rpx;
  height: 60rpx;
  width: 56%;
}

.ratio .students{
  position: absolute;
  margin: 0 auto;
  left: 0;
  color: #fff;
}

.ratio_value{
  font-size: 36rpx;
  padding-top: 10rpx;
  color: #999;
  font-weight: bold;
}

.ratio_value text{
  width: 33.33%;
  display: inline-block;
  text-align: center;
  
}
.ratio_value :first-child{text-align: left;}
.ratio_value :last-child{text-align: right;}
.ratio_value text.male_value, .ratio_value text.female_value{
  width: auto;
}
.male_num, .male_value{
  color: #00688f;
}
.female_num, .female_value{
  color: #a72c34;
}

.employ_rate{
  color: #00bea1;
  font-size: 42rpx;
  font-weight: bold;
  text-align: center;
  /* display: inline; */
  background: #ccf2ec;
  padding: 10rpx 0;
}

/* picker============================== */
.pick_type{
  border: solid #ccc 1rpx;
  border-left: none;
  border-right: none;
  font-size: 32rpx;
  color: #00bea1;
  padding: 20rpx 0;
  margin-top: 15rpx;
}


/* table============ */
.table_head text, .table_tr text{
  float: left; 
}

.table_head, .table_tr{
  padding:0 30rpx;
  height: 90rpx;
  background: #fafafa;
  font-size: 32rpx;
  font-weight: bold;
  color: #333;
  line-height: 90rpx;
  margin-top: 20rpx;
  border:solid #ddd 1rpx;
  border-left: none;
  border-right: none;
}

.table_tr{
  font-weight: normal;
  margin: 0;
  border-top: 0;
  background: #fefefe;
  color: #666;
  border:none;
}
.table_tr.end_tr{
  border-bottom: solid #ddd 1rpx;
  height: 98rpx;
}
.table_tr text{
  border-bottom:solid #ddd 1rpx;
}
.table_tr.end_tr text{
border:none;
}
.view_more{
  color: #00bea1; font-size: 34rpx;
}

.no_more{
  font-size: 28rpx;
  color: #999;
  text-align: center;
  padding: 20rpx;
  padding-top: 0;
  width: 100%;
  display: block;
}

.green_text{color: #00bea1;}

/* filter_bar======================== */
.filter_bar{
  height: 110rpx;
  background: #fafafa;
  position: fixed;
  width: 100%;
  top: 0;
  z-index: 999;
}

.filter_bar view{
  float: left;
  width: 25%;
  text-align: center;
  border-right: solid #ddd 1rpx;
  border-bottom: solid #ddd 1rpx;
  height: 109rpx;
}

.filter_bar_2 view{
  width: 50%;
}

.filter_bar view.selected{
  border-bottom: none;
  background:rgba(255, 255, 255, .95);
}
.filter_bar view:last-of-type{
  border-right: none;
}

.filter_bar view text {
  line-height: 109rpx;
  font-size: 32rpx;
  color: #333;
  /* padding-right: 30rpx;
  background: url(http://p1n6cj41i.bkt.clouddn.com/right_arrow.svg) no-repeat right center; */
}

.filter_bar view>text{
  background: url(http://p1n6cj41i.bkt.clouddn.com/down_arrow.svg) no-repeat right center;
  padding-right: 30rpx;
  background-size: 20rpx;
}
.filter_bar.tab_bar view>text{background: none;}
.filter_bar view.selected>text{
  background: url(http://p1n6cj41i.bkt.clouddn.com/top_arrow.svg) no-repeat right center;
  /* padding-right: 30rpx; */
  background-size: 20rpx;
}
.filter_bar.tab_bar view.selected>text{background: none;}

.filter_bar view text.green_text{color: #00bea1; background: none;}
.filter_content{position: absolute; padding-top: 111rpx; background: rgba(255, 255, 255, .95);
height: 100%;
width: 100%;
margin-bottom: 60rpx;
}

.filter_content scroll-view{
  padding-bottom: 110rpx;
}